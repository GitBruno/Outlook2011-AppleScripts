-- Archive Message for Outlook 2011-- rev 1.1-- Modified by D.Bailey -- added close window-- Original script from http://timmorrow.org/archive-feature-on-outlook-for-mac/comment-page-1/on run {}	-- activate	tell application "Microsoft Outlook"		-- Get the selected message set		set msgList to current messages		if msgList = {} then			error "No messages selected. Select at least one message."			error -128		end if				-- Get the archive folder		set theMsg to item 1 of msgList		set theAccount to account of theMsg		set archiveFolder to folder "Archive" of on my computer				-- Get list of all window IDs		set windowList to every window				-- Iterate through messages		repeat with aMessage in msgList			-- move message into archive folder			move aMessage to archiveFolder			-- if message is in an open window, close the window						repeat with aWindow in windowList				try					if id of aMessage = id of (object of aWindow) then						close aWindow					end if				end try			end repeat		end repeat	end tellend run